<%= form_with(model: service_status, local: true) do |form| %>
  <% if service_status.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(service_status.errors.count, "error") %> prohibited this service_status from being saved:</h2>

      <ul>
        <% service_status.errors.full_messages.each do |message| %>
          <li><%= message %></li>
        <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :service_status, 'Service Status' %>
    <%= form.select :service_status, ['Complete', 'Incomplete'] %>
  </div>

  <div class="actions">
    <%= form.submit 'Create Service Status' %>
  </div>
<% end %>
